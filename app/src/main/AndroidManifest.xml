        <activity android:name=".ui.housingBoard.HousingModifyPostActivity"/>
        <activity android:name=".ui.housingBoard.HousingWritePostActivity"/>
        <activity android:name=".ui.tradingBoard.TradeWritePostActivity"/>
        <activity android:name=".ui.tradingBoard.TradePostViewActivity"/>
        <activity android:name=".ui.tradingBoard.TradeModifyPostActivity"/>
        <activity android:name=".ui.ttingBoard.Button1Activity"/>
        <activity android:name=".ui.ttingBoard.Button2Activity"/>
        <activity android:name=".ui.ttingBoard.Button3Activity"/>
        <activity android:name=".ui.ttingBoard.Button4Activity"/>
        <activity android:name=".ui.ttingBoard.Button5Activity"/>
        <activity android:name=".ui.ttingBoard.Button6Activity"/>
        <activity android:name=".ui.ttingBoard.Button7Activity"/>
        <activity android:name=".ui.ttingBoard.Button8Activity"/>
