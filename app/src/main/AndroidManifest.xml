    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/sujunglogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sujunglogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.login.Login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.main.MainActivity" />
        <activity android:name=".ui.main.NoticeActivity" />
        <activity android:name=".ui.freeBoard.FreeActivity" />
        <activity android:name=".ui.helpBoard.HelpActivity" />
        <activity android:name=".ui.housingBoard.HousingActivity" />
        <activity android:name=".ui.tradingBoard.TradeActivity" />
        <activity android:name=".ui.infoBoard.InfoActivity" />
        <activity android:name=".ui.ttingBoard.TtingActivity" />
        <activity android:name=".ui.register.RegisterActivity" />
        <activity android:name=".ui.freeBoard.FreeWritePostActivity"/>
        <activity android:name=".ui.main.SettingsActivity"/>
        <activity android:name=".ui.main.ProfileActivity"/>
        <activity android:name=".ui.register.Registersub2" />
        <activity android:name=".ui.register.Registersub3" />
        <activity android:name=".ui.register.RegisterEmail1" />
        <activity android:name=".ui.register.RegisterEmail2" />
        <activity android:name=".ui.main.gologin" />
        <activity android:name=".NewActivity" />
        <service
            android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <activity android:name=".ui.main.SettingNicknameActivity" />
        <activity android:name=".ui.main.SettingLocationActivity" />
        <activity android:name=".ui.main.SettingPwdActivity" />
        <activity android:name=".ui.main.SettingDeleteActivity" />
        <service
            android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id"/>
        <activity android:name=".ui.freeBoard.FreePostViewActivity"/>
        <activity android:name=".ui.freeBoard.FreeModifyPostActivity"/>
        <activity android:name=".ui.infoBoard.InfoModifyPostActivity"/>
        <activity android:name=".ui.infoBoard.InfoPostViewActivity"/>
        <activity android:name=".ui.infoBoard.InfoWritePostActivity"/>
        <activity android:name=".ui.helpBoard.HelpModifyPostActivity"/>
